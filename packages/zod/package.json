{
  "name": "@atk/zod",
  "type": "module",
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint --type-aware --deny-warnings --fix",
    "format": "prettier --write .",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:ci": "bun test:coverage"
  },
  "exports": {
    "./package.json": "./package.json",
    "./*": {
      "types": "./src/*.ts",
      "import": "./src/*.ts",
      "require": "./src/*.ts",
      "default": "./src/*.ts"
    }
  },
  "devDependencies": {
    "@tools/typescript-config": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "5.9.3"
  },
  "dependencies": {
    "date-fns": "4.1.0",
    "@orpc/client": "1.10.2",
    "currency-codes": "2.2.0",
    "dnum": "2.15.0",
    "i18n-iso-countries": "7.14.0",
    "viem": "2.38.5",
    "zod": "4.1.12"
  }
}
