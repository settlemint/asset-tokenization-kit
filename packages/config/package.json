{
  "name": "@atk/config",
  "type": "module",
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "lint": "oxlint --type-aware --deny-warnings --fix",
    "format": "prettier --write .",
    "test": "bun test",
    "test:coverage": "bun test --coverage",
    "test:ci": "bun test:coverage"
  },
  "exports": {
    "./*": {
      "import": "./src/*.ts",
      "types": "./src/*.ts"
    }
  },
  "devDependencies": {
    "@tools/typescript-config": "workspace:*",
    "@types/bun": "latest"
  },
  "peerDependencies": {
    "typescript": "5.9.3"
  },
  "dependencies": {
    "@t3-oss/env-core": "0.13.8",
    "zod": "4.1.12"
  }
}
