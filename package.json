{
  "name": "@settlemint/starterkit-asset-tokenization",
  "description": "SettleMint Starter Kit - Asset Tokenization",
  "version": "0.4.8",
  "private": false,
  "license": "FSL-1.1-MIT",
  "author": {
    "name": "SettleMint",
    "email": "support@settlemint.com",
    "url": "https://settlemint.com"
  },
  "homepage": "https://github.com/settlemint/sdk/blob/main/apps/asset-tokenization/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settlemint/sdk.git",
    "directory": "apps/asset-tokenization"
  },
  "bugs": {
    "url": "https://github.com/settlemint/sdk/issues",
    "email": "support@settlemint.com"
  },
  "scripts": {
    "clean": "git clean -fdX && find . -type f \\( -name 'pnpm-lock.yaml' -o -name 'package-lock.json' -o -name 'yarn.lock' -o -name 'bun.lockb' \\) -exec rm -f {} + && bun install",
    "knip": "knip",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "codegen": "turbo codegen",
    "format": "turbo format",
    "connect": "turbo connect",
    "test": "turbo test",
    "compile": "turbo compile",
    "postinstall": "lefthook install"
  },
  "devDependencies": {
    "@settlemint/sdk-cli": "0.4.11",
    "@biomejs/biome": "1.9.1",
    "@types/bun": "1.1.9",
    "lefthook": "1.7.15",
    "turbo": "2.1.2",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": ">=18"
  },
  "workspaces": ["apps/*", "packages/*"],
  "trustedDependencies": [
    "@biomejs/biome",
    "@settlemint/starterkit-asset-tokenization-contracts",
    "bufferutil",
    "keccak",
    "lefthook",
    "protobufjs",
    "secp256k1",
    "utf-8-validate"
  ],
  "files": [
    "/.github",
    "/.prompts",
    "/.vscode",
    "/apps",
    "/.cursorrules",
    "/.gitignore",
    "/.node-version",
    "/.npmrc",
    "/biome.json",
    "/knip.json",
    "/lefthook.yml",
    "/LICENSE",
    "/logo.svg",
    "/package.json",
    "/README.md",
    "/turbo.json",
    "/docker-compose.yml",
    "/docker-entrypoint.sh",
    "/Dockerfile",
    "!/apps/*/.next",
    "!/apps/*/.next/**",
    "!/apps/*/.settlemint",
    "!/apps/*/.settlemint/**",
    "!/apps/**/.env.local",
    "!/apps/**/settlemint.config.mjs",
    "!/apps/**/.settlemint/**",
    "!/apps/contracts/cache_forge/**",
    "!/apps/contracts/out/**",
    "!/apps/**/node_modules",
    "!/apps/**/node_modules/**",
    "!/apps/contracts/dependencies",
    "!/apps/contracts/dependencies/**",
    "!apps/**/.turbo",
    "!apps/**/.turbo/**",
    "!apps/subgraph/build",
    "!apps/subgraph/build/**",
    "!apps/subgraph/generated",
    "!apps/subgraph/generated/**"
  ]
}
