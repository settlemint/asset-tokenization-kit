  "private": false,
  "license": "FSL-1.1-MIT",
  "author": {
    "name": "SettleMint",
    "email": "support@settlemint.com",
    "url": "https://settlemint.com"
  },
  "homepage": "https://github.com/settlemint/starterkit-asset-tokenization/blob/main/apps/dapp/README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/settlemint/starterkit-asset-tokenization.git"
  },
  "bugs": {
    "url": "https://github.com/settlemint/starterkit-asset-tokenization/issues",
    "email": "support@settlemint.com"
  },
  "files": [
    ".github",
    ".vscode",
    "kit",
    "src",
    ".gitignore",
    "biome.json",
    "bunfig.toml",
    "lefthook.yml",
    "LICENSE",
    "logo.svg",
    "package.json",
    "README.md",
    "turbo.json"
  ],
  "scripts": {
    "clean": "git clean -fdX && find . -type f \\( -name 'pnpm-lock.yaml' -o -name 'package-lock.json' -o -name 'yarn.lock' -o -name 'bun.lockb' \\) -exec rm -f {} + && bun install",
    "build": "turbo build",
    "dev": "turbo dev",
    "lint": "turbo lint",
    "test": "turbo test",
    "test:deploy": "turbo test:deploy",
    "check-types": "turbo check-types",
    "publish-npm": "bun publish --tag ${TAG} --access public || exit 0",
    "publish-docker": "turbo publish-docker",
    "deploy": "turbo deploy",
    "codegen": "turbo codegen",
    "lint:biome": "biome lint --write --unsafe .",
    "format:biome": "biome format --write ."
  },
  "dependencies": {},
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "lefthook": "1.8.5",
    "turbo": "2.3.3",
    "typescript": "5.7.2",
    "ultracite": "4.1.8"
  },
  "trustedDependencies": ["@biomejs/biome", "lefthook"],
  "packageManager": "bun@1.1.38",
  "workspaces": ["kit/*"],
  "overrides": {
    "react-is": "19.0.0-rc-66855b96-20241106",
    "elliptic": "6.6.0",
    "ws": "7.5.10"
  }
}
