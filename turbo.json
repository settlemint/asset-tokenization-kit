{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "tasks": {
    "ci": {
      "dependsOn": ["compile", "codegen", "lint", "build"]
    },
    "ci:local": {
      "dependsOn": ["format", "ci"]
    },
    "build": {
      "dependsOn": ["codegen"]
    },
    "codegen": {
      "dependsOn": ["dependencies"]
    },
    "compile": {
      "dependsOn": ["dependencies"]
    },
    "dependencies": {},
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {},
    "lint": {}
  }
}
