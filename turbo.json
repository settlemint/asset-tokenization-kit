{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "envMode": "loose",
  "tasks": {
    "build": {
      "dependsOn": [
        "codegen",
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        ".env*"
      ],
      "outputs": [
        ".next/**",
        "!.next/cache/**"
      ]
    },
    "lint": {
      "dependsOn": [
        "codegen",
        "^lint"
      ]
    },
    "check-types": {
      "dependsOn": [
        "^check-types"
      ]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "publish-npm": {
      "dependsOn": [
        "^publish-npm"
      ],
      "cache": false
    },
    "publish-docker": {
      "dependsOn": [
        "codegen",
        "^publish-docker"
      ],
      "cache": false
    },
    "deploy": {
      "dependsOn": [
        "^deploy"
      ],
      "cache": false
    },
    "codegen": {
      "dependsOn": [
        "^codegen"
      ],
      "cache": false
    }
  }
}