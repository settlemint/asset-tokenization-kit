{
  "$schema": "https://turbo.build/schema.json",
  "ui": "tui",
  "envMode": "loose",
  "tasks": {
    "ci": {
      "dependsOn": ["format", "compile", "codegen", "lint", "build"]
    },
    "ci:publish": {
      "dependsOn": ["package"]
    },
    "//#version": {
      "cache": false
    },
    "build": {
      "dependsOn": ["codegen"]
    },
    "codegen": {
      "dependsOn": ["dependencies"]
    },
    "compile": {
      "dependsOn": ["dependencies"]
    },
    "package": {
      "cache": false,
      "dependsOn": ["//#version"]
    },
    "dependencies": {},
    "format": {},
    "lint": {}
  }
}
