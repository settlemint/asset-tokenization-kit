{
  "name": "@atk/artifacts",
  "version": "2.0.0",
  "private": true,
  "description": "Shared artifacts workspace for cross-workspace outputs",
  "type": "module",
  "scripts": {
    "clean": "rm -rf ./portal ./genesis ./postgres ./docker-compose",
    "artifacts:contracts": "bun run ../contracts/tools/artifacts-abi.ts && bun run ../contracts/tools/artifacts-genesis.ts",
    "artifacts:charts": "bun run ../charts/tools/copy-artifacts.ts",
    "artifacts:docker-compose": "bun run ./tools/update-docker-compose.ts",
    "artifacts:database": "bun run ./tools/export-database.ts",
    "artifacts": "bun run artifacts:contracts && bun run artifacts:charts && bun run artifacts:docker-compose"
  },
  "devDependencies": {
    "js-yaml": "4.1.0"
  }
}