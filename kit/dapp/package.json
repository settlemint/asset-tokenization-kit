{
  "name": "dapp",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "translate": "languine translate",
    "dev": "bun --bun vite dev",
    "typecheck": "tsc --noEmit",
    "watch": "tsc --noEmit --watch",
    "build": "bun --bun vite build",
    "shadcn": "bunx --bun shadcn@canary",
    "codegen:settlemint": "settlemint codegen",
    "codegen:gql-tada": "gql-tada turbo",
    "format": "prettier -l --write .",
    "lint": "eslint . --max-warnings 0",
    "lint:fix": "eslint . --fix",
    "publish": "DOCKER_BUILD_TAG=$(jq -r .version ../../package.json); if [ -z \"$CI\" ]; then DOCKER_BUILD_TAG=\"$DOCKER_BUILD_TAG-dev.$(date +%s)\"; fi; docker buildx build . --platform=linux/amd64,linux/arm64 --provenance true --sbom true -t ghcr.io/settlemint/asset-tokenization-kit:$DOCKER_BUILD_TAG --push",
    "db:check": "drizzle-kit check",
    "db:push": "drizzle-kit push",
    "db:export": "(echo \"\\c hasura;\" | tee ../../tools/docker/postgres/atk.sql) && (drizzle-kit export | tee -a ../../tools/docker/postgres/atk.sql)",
    "postdb:push": "settlemint hasura track -a",
    "db:studio": "drizzle-kit studio",
    "db:auth:generate": "bunx --bun @better-auth/cli generate -y --config=src/lib/auth/index.ts --output=src/lib/db/schemas/auth.ts"
  },
  "devDependencies": {
    "@eslint/js": "9.29.0",
    "@fontsource-variable/figtree": "5.2.8",
    "@fontsource-variable/roboto-mono": "5.2.6",
    "@tanstack/eslint-plugin-query": "5.78.0",
    "@tanstack/eslint-plugin-router": "1.121.21",
    "@types/bun": "1.2.16",
    "@types/react": "19.1.8",
    "@types/react-dom": "19.1.6",
    "dotenv": "16.5.0",
    "eslint": "9.29.0",
    "eslint-plugin-react": "7.37.5",
    "globals": "16.2.0",
    "languine": "3.1.4",
    "tw-animate-css": "1.3.4",
    "typescript": "5.8.3",
    "typescript-eslint": "8.34.1",
    "vite": "6.3.5",
    "vite-tsconfig-paths": "5.1.4"
  },
  "peerDependencies": {},
  "dependencies": {
    "@daveyplate/better-auth-tanstack": "1.3.6",
    "@daveyplate/better-auth-ui": "2.0.8",
    "@orpc/client": "1.5.2",
    "@orpc/openapi": "1.5.2",
    "@orpc/react-query": "1.5.2",
    "@orpc/server": "1.5.2",
    "@orpc/tanstack-query": "1.5.2",
    "@orpc/zod": "1.5.2",
    "@radix-ui/react-dialog": "1.1.14",
    "@radix-ui/react-dropdown-menu": "2.1.15",
    "@radix-ui/react-progress": "1.1.7",
    "@radix-ui/react-slot": "1.2.3",
    "@settlemint/sdk-blockscout": "2.4.0",
    "@settlemint/sdk-hasura": "2.4.0",
    "@settlemint/sdk-ipfs": "2.4.0",
    "@settlemint/sdk-minio": "2.4.0",
    "@settlemint/sdk-portal": "2.4.0",
    "@settlemint/sdk-thegraph": "2.4.0",
    "@settlemint/sdk-utils": "2.4.0",
    "@t3-oss/env-core": "0.13.8",
    "@tailwindcss/postcss": "4.1.10",
    "@tanstack/react-query": "5.80.7",
    "@tanstack/react-query-devtools": "5.80.7",
    "@tanstack/react-router": "1.121.21",
    "@tanstack/react-router-devtools": "1.121.21",
    "@tanstack/react-router-with-query": "1.121.21",
    "@tanstack/react-start": "1.121.21",
    "@tanstack/router-devtools": "1.121.21",
    "better-auth": "1.2.9",
    "class-variance-authority": "0.7.1",
    "clsx": "2.1.1",
    "drizzle-orm": "0.44.2",
    "i18n-iso-countries": "7.14.0",
    "i18next": "25.2.1",
    "lucide-react": "0.516.0",
    "next-themes": "0.4.6",
    "postcss": "8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-i18next": "15.5.3",
    "sonner": "2.0.5",
    "superjson": "2.2.2",
    "tailwind-merge": "3.3.1",
    "tailwindcss": "4.1.10",
    "zod": "3.25.67"
  },
  "overrides": {
    "@tanstack/react-query": "5.80.7",
    "@tanstack/react-query-devtools": "5.80.7",
    "@tanstack/react-router": "1.121.21",
    "@tanstack/react-router-devtools": "1.121.21",
