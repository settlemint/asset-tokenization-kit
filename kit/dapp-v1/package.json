{
  "name": "dapp-v1",
  "version": "1.0.6",
  "private": false,
  "license": "FSL-1.1-MIT",
  "scripts": {
    "xcompile": "tsc --noEmit",
    "xwatch": "tsc --noEmit --watch",
    "xcodegen:settlemint": "settlemint codegen",
    "xcodegen:gql-tada": "gql-tada turbo",
    "xformat": "prettier --experimental-cli --write .",
    "xlint": "next lint",
    "xbuild": "next build --turbopack",
    "xdev": "next dev --turbopack",
    "xpublish": "echo 'Docker builds come here when the dapp builds again'",
    "xprod:local": "next build --turbopack && cp -r .next/static .next/standalone/kit/dapp/.next/static && cp -r public .next/standalone/public && node .next/standalone/kit/dapp/server.js",
    "xstart": "next start",
    "xtest": "bun test",
    "xdb:check": "drizzle-kit check",
    "xdb:push": "drizzle-kit push",
    "xdb:export": "(echo \"\\c hasura;\" | tee ../../tools/docker/postgres/atk.sql) && (drizzle-kit export | tee -a ../../tools/docker/postgres/atk.sql)",
    "xpostdb:push": "settlemint hasura track -a",
    "xdb:studio": "drizzle-kit studio"
  },
  "dependencies": {},
  "devDependencies": {},
  "peerDependencies": {},
  "trustedDependencies": [],
  "overrides": {}
}
